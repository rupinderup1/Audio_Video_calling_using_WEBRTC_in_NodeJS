<body>
    <% const m=getMessages() %>
    <% if (m.success) { %>
        <div class="alert alert-success"><%= m.success %></div>
    <% } %>
    <% if (m.error) { %>
        <div class="alert alert-danger"><%= m.error %></div>
    <% } %>